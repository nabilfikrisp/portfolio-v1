---
import type { HTMLAttributes } from "astro/types";

type Props = HTMLAttributes<"svg">;
const {
  width = 24,
  height = 24,
  fill = "none",
  stroke = "currentColor",
  "stroke-width": strokeWidth = 2,
  class: className,
  ...props
} = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={width}
  height={height}
  viewBox="0 0 24 24"
  fill={fill}
  stroke={stroke}
  stroke-width={strokeWidth}
  stroke-linecap="round"
  stroke-linejoin="round"
  class:list={["lucide lucide-map-pin", className]}
  ><path d="M14 2v4a2 2 0 0 0 2 2h4"></path><path d="M15 18a3 3 0 1 0-6 0"
  ></path><path d="M15 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V7z"
  ></path><circle cx="12" cy="13" r="2"></circle></svg
>
